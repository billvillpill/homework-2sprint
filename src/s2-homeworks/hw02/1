const filterAffairs = <T extends AffairPriorityType>(affairs: AffairType[], type: T): AffairType[] => { return affairs.filter(({ priority }) => priority === type); }; export const getFilteredAffairs = (affairs: AffairType[], filterType: FilterType): AffairType[] => { switch (filterType) { case ‘all’: return affairs; default: return filterAffairs(affairs, filterType as AffairPriorityType); } }; export default getFilteredAffairs(defaultAffairs, ‘high’);